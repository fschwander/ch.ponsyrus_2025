{
  "semi": false,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "trailingComma": "all",
  "proseWrap": "always",
  "importOrder": ["^@(.*)", "^[./](.*)(?<!scss)$", "^[./](.*)(scss)$"],
  "importOrderSeparation": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}